{
  "variables": {
    "core": "https://api.density.io/v1"
  },
  "resources": {
    "spaces": {
      "list": {
        "method": "GET",
        "url": "{{{core}}}/{{organisation}}/spaces/",
        "desc": "List all spaces.",
        "qs": {
          "detail": "{{detail}}"
        }
      },
      "get": {
        "method": "GET",
        "url": "{{{core}}}/{{organisation}}/spaces/{{id}}/",
        "desc": "Get a space by id."
      },
      "create": {
        "method": "POST",
        "url": "{{{core}}}/{{organisation}}/spaces/",
        "desc": "Create a space.",
        "headers": {
          "Content-Type": "application/json",
          "Accept": "application/json"
        },
        "body": {
          "name": "{{{name}}}",
          "description": "{{{description}}}",
          "timezone": "{{{timezone}}}",
          "daily_reset": "{{{daily_reset}}}"
        }
      },
      "update": {
        "method": "PUT",
        "url": "{{{core}}}/{{organisation}}/spaces/{{id}}/",
        "desc": "Update a space.",
        "headers": {
          "Content-Type": "application/json",
          "Accept": "application/json"
        },
        "body": {
          "name": "{{{name}}}",
          "description": "{{{description}}}",
          "time_zone": "{{{time_zone}}}",
          "daily_reset": "{{{daily_reset}}}",
          "capacity": "{{{capacity}}}"
        }
      },
      "delete": {
        "method": "DELETE",
        "url": "{{{core}}}/{{organisation}}/spaces/{{id}}",
        "desc": "Delete a space by id.",
        "headers": {
          "Content-Type": "application/json"
        },
        "body": {
          "name": "{{name}}"
        }
      },
      "counts": {
        "method": "GET",
        "url": "{{{core}}}/{{organisation}}/spaces/{{id}}/counts/",
        "desc": "Query counts over time for a space, at specified intervals. Includes analytics specifying the minimum and maximum count that was recorded in each interval.",
        "qs": {
          "start_time": "{{start_time}}",
          "end_time": "{{end_time}}",
          "interval": "{{interval}}",
          "page_size": "{{page_size}}",
          "page": "{{page}}",
          "order": "{{order}}"
        }
      },
      "allCounts": {
        "method": "GET",
        "url": "{{{core}}}/{{organisation}}/spaces/counts/",
        "desc": "Query counts over time for all spaces in the organization, at specified intervals. Includes analytics specifying the minimum and maximum count that was recorded in each interval.",
        "qs": {
          "start_time": "{{start_time}}",
          "end_time": "{{end_time}}",
          "interval": "{{interval}}",
          "page_size": "{{page_size}}",
          "page": "{{page}}",
          "order": "{{order}}"
        }
      },
      "events": {
        "method": "GET",
        "url": "{{{core}}}/{{organisation}}/spaces/{{id}}/events/",
        "desc": "Query individual traffic events over time for a space.",
        "qs": {
          "start_time": "{{start_time}}",
          "end_time": "{{end_time}}",
          "doorway_id": "{{doorway_id}}",
          "direction": "{{direction}}",
          "page_size": "{{page_size}}",
          "page": "{{page}}",
          "order": "{{order}}"
        }
      },
      "reset": {
        "method": "POST",
        "url": "{{{core}}}/{{organisation}}/spaces/{{id}}/resets/",
        "headers": {
          "Content-Type": "application/json",
          "Accept": "application/json"
        },
        "body": {
          "count": "{{count}}",
          "timestamp": "{{timestamp}}"
        },
        "desc": "Creates a reset on a space."
      },
      "csv": {
        "method": "GET",
        "url": "{{{core}}}/{{organisation}}/spaces/{{id}}/csv/",
        "desc": "Get CSV event for a given space.",
        "qs": {
          "doorway_id": "{{doorway_id}}",
          "start_time": "{{start_time}}",
          "end_time": "{{end_time}}"
        }
      }
    },
    "doorways": {
      "list": {
        "method": "GET",
        "url": "{{{core}}}/{{organisation}}/doorways/",
        "desc": "List all doorways.",
        "qs": {
          "detail": "{{detail}}",
          "environment": "{{#environment}}True{{/environment}}"
        }
      },
      "get": {
        "method": "GET",
        "url": "{{{core}}}/{{organisation}}/doorways/{{id}}/",
        "desc": "Get a doorway by id."
      },
      "create": {
        "method": "POST",
        "url": "{{{core}}}/{{organisation}}/doorways/",
        "desc": "Create a doorway.",
        "headers": {
          "Content-Type": "application/json",
          "Accept": "application/json"
        },
        "body": {
          "name": "{{name}}",
          "description": "{{description}}",
          "sensor_id": "{{sensor_id}}",
          "environment": {
            "width": "{{width}}",
            "height": "{{height}}",
            "clearance": "{{clearance}}",
            "power_type": "{{power_type}}"
          }
        }
      },
      "update": {
        "method": "PUT",
        "url": "{{{core}}}/{{organisation}}/doorways/{{id}}/",
        "desc": "Update a doorway.",
        "headers": {
          "Content-Type": "application/json",
          "Accept": "application/json"
        },
        "body": {
          "name": "{{name}}",
          "description": "{{description}}",
          "sensor_id": "{{sensor_id}}",
          "environment": {
            "width": "{{width}}",
            "height": "{{height}}",
            "clearance": "{{clearance}}",
            "power_type": "{{power_type}}"
          }
        }
      },
      "delete": {
        "method": "DELETE",
        "url": "{{{core}}}/{{organisation}}/doorways/{{id}}",
        "desc": "Delete a doorway by id."
      }
    },
    "events": {
      "list": {
        "method": "GET",
        "url": "{{{core}}}/{{organisation}}/events/",
        "qs": {
          "start_time": "{{start_time}}",
          "end_time": "{{end_time}}",
          "space_id": "{{space_id}}",
          "doorway_id": "{{doorway_id}}",
          "sensor_id": "{{sensor_id}}",
          "page_size": "{{page_size}}",
          "page": "{{page}}"
        },
        "desc": "List all events. Takes a 'space_id', 'start_time', 'end_time', and 'doorway_id'"
      }
    },
    "resets": {
      "list": {
        "method": "GET",
        "url": "{{{core}}}/{{organisation}}/resets/",
        "qs": {
          "start_time": "{{start_time}}",
          "end_time": "{{end_time}}",
          "space_id": "{{space_id}}",
          "page_size": "{{page_size}}",
          "page": "{{page}}"
        },
        "desc": "List all events. Takes a 'space_id', 'start_time', 'end_time', and 'doorway_id'"
      }
    },
    "sensors": {
      "list": {
        "method": "GET",
        "url": "{{{core}}}/{{organisation}}/sensors/",
        "qs": {
          "start_time": "{{start_time}}",
          "end_time": "{{end_time}}",
          "space_id": "{{id}}",
          "shadow": "{{shadow}}"
        },
        "desc": "List all sensors."
      },
      "get": {
        "method": "GET",
        "url": "{{{core}}}/{{organisation}}/sensors/{{id}}",
        "desc": "Get a sensor by id."
      },
      "delete": {
        "method": "DELETE",
        "url": "{{{core}}}/{{organisation}}/sensors/{{id}}",
        "desc": "Delete a sensor by id."
      }
    },
    "links": {
      "list": {
        "method": "GET",
        "url": "{{{core}}}/{{organisation}}/links/",
        "desc": "List all links.",
        "qs": {
          "space_id": "{{space_id}}",
          "doorway_id": "{{doorway_id}}"
        }
      },
      "get": {
        "method": "GET",
        "url": "{{{core}}}/{{organisation}}/links/{{id}}/",
        "desc": "Get a link by id."
      },
      "create": {
        "method": "POST",
        "url": "{{{core}}}/{{organisation}}/links/",
        "desc": "Create a new link.",
        "headers": {
          "Content-Type": "application/json",
          "Accept": "application/json"
        },
        "body": {
          "space_id": "{{space_id}}",
          "doorway_id": "{{doorway_id}}",
          "sensor_placement": "{{sensor_placement}}"
        }
      },
      "update": {
        "method": "PUT",
        "url": "{{{core}}}/{{organisation}}/links/{{id}}/",
        "desc": "Update a link's sensor placement.",
        "headers": {
          "Content-Type": "application/json",
          "Accept": "application/json"
        },
        "body": {
          "sensor_placement": "{{sensor_placement}}"
        }
      },
      "delete": {
        "method": "DELETE",
        "url": "{{{core}}}/{{organisation}}/links/{{id}}/",
        "desc": "Delete a link."
      }
    },
    "webhooks": {
      "list": {
        "method": "GET",
        "url": "{{{core}}}/{{organisation}}/webhooks/",
        "desc": "List all webhooks."
      },
      "get": {
        "method": "GET",
        "url": "{{{core}}}/{{organisation}}/webhooks/{{id}}/",
        "desc": "Get a webhook by id."
      },
      "create": {
        "method": "POST",
        "url": "{{{core}}}/{{organisation}}/webhooks/",
        "desc": "Create a new webhook.",
        "headers": {
          "Content-Type": "application/json",
          "Accept": "application/json"
        },
        "body": {
          "name": "{{{name}}}",
          "description": "{{{description}}}",
          "endpoint": "{{{endpoint}}}"
        }
      },
      "update": {
        "method": "PUT",
        "url": "{{{core}}}/{{organisation}}/webhooks/{{id}}/",
        "desc": "Update a webhook's endpoint.",
        "headers": {
          "Content-Type": "application/json",
          "Accept": "application/json"
        },
        "body": {
          "name": "{{{name}}}",
          "description": "{{{description}}}",
          "endpoint": "{{{endpoint}}}"
        }
      },
      "delete": {
        "method": "DELETE",
        "url": "{{{core}}}/{{organisation}}/webhooks/{{id}}/",
        "desc": "Delete a webhook."
      }
    },
    "sockets": {
      "create": {
        "method": "POST",
        "url": "{{{core}}}/sockets",
        "desc": "Create a new socket connection.",
        "headers": {
          "Content-Type": "application/json",
          "Accept": "application/json"
        }
      }
    }
  }
}
